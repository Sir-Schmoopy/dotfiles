ln -fs ~/dotfiles/bashrc          ~/.bash_profile
ln -fs ~/dotfiles/inputrc         ~/.inputrc
ln -fs ~/dotfiles/tmux.conf       ~/.tmux.conf
ln -fs ~/dotfiles/tmux.conf.local ~/.tmux.conf.local
ln -fs ~/dotfiles/vimconfig/vimrc ~/.vimrc
ln -fs ~/dotfiles/vimconfig/vim   ~/.vim
